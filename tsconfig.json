{
  "compilerOptions": {
    "target": "ES2016",
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "dist",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
  },
  "include": ["src/**/*"]
}
